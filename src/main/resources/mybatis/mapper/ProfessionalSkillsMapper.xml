<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zdxh.personnelmanage.mapper.ProfessionalSkillsMapper">


    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.zdxh.personnelmanage.po.ProfessionalSkills">
        <id column="ps_id" property="psId" />
        <result column="ps_info" property="psInfo" />
        <result column="ps_level" property="psLevel" />
        <result column="ps_issue_time" property="psIssueTime" />
        <result column="ps_number" property="psNumber" />
        <result column="emp_id" property="empId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ps_id, ps_info, ps_level, ps_issue_time, ps_number, emp_id
    </sql>
    <select id="findAllProfessionalSkillsBySearch" parameterType="string" resultType="cn.zdxh.personnelmanage.po.ProfessionalSkills">
        SELECT * FROM professional_skills WHERE ps_info LIKE #{parame} OR ps_level LIKE #{parame} OR ps_number LIKE #{parame}
    </select>

</mapper>
